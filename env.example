# Cloudflare AI Configuration
# Copy this file to .env and fill in your actual values

# Cloudflare Account ID (required for all AI features)
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here

# Cloudflare API Token (required for all AI features)
# Must have AI permissions enabled
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here

# Optional: Cloudflare AutoRAG Endpoint
AUTORAG_ENDPOINT=https://api.cloudflare.com/client/v4/accounts

# Optional: Node.js version for build environment
NODE_VERSION=18.17.1
